{
  "family": "my-task-definition",
  "containerDefinitions": [
    {
      "name": "go-app",
      "image": "666607936402.dkr.ecr.ap-northeast-1.amazonaws.com/terraform-project:latest",
      "memory": 128,
      "environment": [
        {
          "name": "LOG_LEVEL",
          "value": "info"
        }
      ],
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
